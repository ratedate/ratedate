<!DOCTYPE html>
<html>
<head>
  <title>Ratedate ICO</title>
  <%= csrf_meta_tags %>
  <%= meta_tags %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => 'reload' %>
  <%= google_analytics %>
  <%= ya_metric %>
</head>

<body>
<div class="container-fluid container-start">
  <div class="row justify-content-md-center">
    <div class="col-md-4 text-center">
      <div class="nav justify-content-center">
        <a href="http://ico.ratedate.net" class="nav-link"><span class="icon-logo text-light big-logo"></span></a>
      </div>
    </div>
  </div>
  <div class="row justify-content-md-center">
    <div class="col-xl-3 col-lg-4 col-md-6 justify-content-center pt-3 text-light text-center">
      <%= yield %>
    </div>
  </div>
</div>
</body>
</html>
